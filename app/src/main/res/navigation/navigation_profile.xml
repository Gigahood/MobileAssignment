<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_profile"
    app:startDestination="@id/profileMain">

    <fragment
        android:id="@+id/profileMain"
        android:name="com.example.drugassignment.Profile_Module.ProfileMain"
        android:label="fragment_profile_main"
        tools:layout="@layout/fragment_profile_main" >
        <action
            android:id="@+id/action_profileMain_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>

    <activity
        android:id="@+id/profile_Activity"
        android:name="com.example.drugassignment.Profile_Module.ProfileMain"
        android:label="activity_main"
        tools:layout="@layout/activity_main">
    </activity>

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.drugassignment.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/editProgile"
        android:name="com.example.drugassignment.Profile_Module.EditProgile"
        android:label="fragment_edit_progile"
        tools:layout="@layout/fragment_edit_progile" >
        <action
            android:id="@+id/action_editProgile_to_profile_Activity"
            app:destination="@id/profile_Activity" />
    </fragment>

</navigation>